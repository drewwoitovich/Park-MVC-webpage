@model IEnumerable<Capstone.Web.Models.Survey>

@{
    ViewBag.Title = "SurveyResults";
}

<h2>Survey Results</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ParkCode)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EmailAddress)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.State)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ActivityLevel)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.ParkCode)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.EmailAddress)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.State)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ActivityLevel)
        </td>
    </tr>
}

</table>
